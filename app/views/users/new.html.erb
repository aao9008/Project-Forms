<form action="/users" method="post" accept-charset="UTF-8" data-turbo="true">
    <div>
        <label for="username">Username</label>
        <input type="text" name="user[username]" id="username" autocomplete="new-username">
    </div>

    <div>
        <label for="email">Email</label>
        <input type="email" name="user[email]" id="email">
    </div>

    <div>
        <label for="password">Password</label>
        <input type="password" name="user[email]" id="password" autocomplete="new-password">
    </div>

    <div>
        <button type=submit>Create User</button>
    </div>
</form>